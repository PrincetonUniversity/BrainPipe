#!/bin/env bash
#
#SBATCH -p all                # partition (queue)
#SBATCH -c 12                      # number of cores
#SBATCH -t 60                # time (minutes)
#SBATCH -o /jukebox/scratch/zmd/logs/patch_%j.out        # STDOUT #add _%a to see each array job
#SBATCH -e /jukebox/scratch/zmd/logs/patch_%j.err        # STDERR #add _%a to see each array job
#SBATCH --contiguous #used to try and get cpu mem to be contigous
#SBATCH --mem 50000 #50 gbs

echo "In the directory: `pwd` "
echo "As the user: `whoami` "
echo "on host: `hostname` "

cat /proc/$$/status | grep Cpus_allowed_list

module load anacondapy/5.1.0
. activate lightsheet

python run_patch.py /jukebox/wang/pisano/tracing_output/antero_4x/20180327_jg40_bl6_sim_03

